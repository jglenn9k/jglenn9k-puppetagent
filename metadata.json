{
  "name": "thedonkdonk-puppetagent",
  "version": "0.1.0",
  "author": "thedonkdonk",
  "summary": "Installs and manages the Puppet agent.",
  "license": "Apache-2.0",
  "source": "git://github.com/thedonkdonk/thedonkdonk-puppetagent.git",
  "project_page": "https://github.com/thedonkdonk/thedonkdonk-puppetagent",
  "issues_url": "https://github.com/thedonkdonk/thedonkdonk-puppetagent/issues",
  "operatingsystem_support": [
  	{
      "operatingsystem": "Darwin",
      "operatingsystemrelease": ["14.3.0"]
    },
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": ["2008","2008 R2","2012","2012 R2"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5.0","6.0","7.0"]
    },
    { 
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5.0","6.0","7.0"]
    },
    { 
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["6.0","7.0"]
    },
    { 
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04"]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.0.0 <5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.4.0 <2.0.0"
    },
    {
      "name": "puppet/download_file",
      "version_requirement": ">= 1.2.1"
    }
  ]
}

